{
    "name": "enobrev/api",
    "license": "MIT",
    "version": "0.9.0-dev",
    "description": "REST-Like API Library",
    "type": "library",
    "authors": [
        {
            "name": "Mark Armendariz",
            "email": "src@enobrev.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url":  "https://github.com/enobrev/orm"
        },
        {
            "type": "vcs",
            "url":  "https://github.com/enobrev/tools"
        }
    ],
    "require": {
        "php": ">=7.0.10",
        "enobrev/tools":  "dev-master",
        "enobrev/orm":  "dev-master",
        "zendframework/zend-diactoros": "^1.3"
    },
    "require-dev": {
        "monolog/monolog": "^1.17",
        "phpunit/phpunit": "^5.1",
        "twig/twig": "^1.24",
        "nategood/commando": "^0.2.8"
    },
    "autoload": {
        "psr-4": {
            "Enobrev\\API\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Enobrev\\API\\Mock\\": "tests/Mock/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "bin": [
        "bin/generate_data_map.php"
    ]
}
